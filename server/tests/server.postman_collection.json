		{
			"name": "PostReviews",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 201\", function () {",
							"    pm.response.to.have.status(201);",
							"});",
							"",
							"pm.test(\"Response is an array of created reviews\", function () {",
							"    const response = pm.response.json();",
							"    pm.expect(Array.isArray(response)).to.be.true;",
							"});",
							"",
							"pm.test(\"First review has all required fields\", function () {",
							"    const response = pm.response.json();",
							"    if (response.length > 0) {",
							"        const review = response[0];",
							"        pm.expect(review).to.have.property('personName');",
							"        pm.expect(review).to.have.property('reviewType');",
							"        pm.expect(review).to.have.property('_id');",
							"        pm.expect(review).to.have.property('createdAt');",
							"        ",
							"        pm.collectionVariables.set(\"reviewId\", review._id);",
							"    }",
							"});",
							"",
							"pm.test(\"Review type-specific fields are correct\", function () {",
							"    const response = pm.response.json();",
							"    if (response.length > 0) {",
							"        const review = response[0];",
							"        if (review.reviewType === 'website') {",
							"            pm.expect(review).to.have.property('websiteReview');",
							"            pm.expect(review.websiteReview).to.be.within(1, 5);",
							"        } else if (review.reviewType === 'museum') {",
							"            pm.expect(review).to.have.property('museumReview');",
							"            pm.expect(review.museumReview).to.be.a('string');",
							"        }",
							"    }",
							"});",
							"pm.test(\"Save review ID for later tests\", function () {",
							"    const response = pm.response.json();",
							"    if (response.length > 0 && response[0]._id) {",
							"        pm.collectionVariables.set(\"reviewId\", response[0]._id);",
							"        console.log(\"Saved reviewId:\", response[0]._id);",
							"    }",
							"});"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
